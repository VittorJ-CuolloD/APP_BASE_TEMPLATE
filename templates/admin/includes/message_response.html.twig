{% for flashMessage in app.flashes('success') %}
    <script>
        Swal.fire({
            icon: 'success',
            title: 'Completado!',
            text: '{{ flashMessage }}',
            showConfirmButton: false,
            timer:3000
        });
    </script>
{% endfor %}
